@import url("https://fonts.googleapis.com/css2?family=Inter:wght@400;600;700&display=swap");

@tailwind base;
@tailwind components;
@tailwind utilities;

html {
  @apply bg-black text-white;
}

a {
  @apply underline hover:no-underline;
}

input {
  @apply border rounded shadow-inner bg-transparent;
}
